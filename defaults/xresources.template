Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight
urxvt*font: xft:DejaVu Sans Mono:size=10
urxvt*boldFont: xft:DejaVu Sans Mono:size=14
urxvt*italicFont: xft:DejaVu Sans Mono:size=14 
urxvt*bolditalicFont: xft:DejaVu Sans Mono:size=14 
